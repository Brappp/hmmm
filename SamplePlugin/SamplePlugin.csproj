<Project Sdk="Dalamud.NET.SDK/11.0.0">
  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <RootNamespace>OceanFishingAutomator</RootNamespace>
    <AssemblyName>OceanFishingAutomator</AssemblyName>
    <Version>1.0.0.0</Version>
    <Description>Automates ocean fishing using tug reading and dynamic route adaptation.</Description>
    <PackageProjectUrl>https://github.com/yourname/OceanFishingAutomator</PackageProjectUrl>
    <PackageLicenseExpression>AGPL-3.0-or-later</PackageLicenseExpression>
    <IsPackable>false</IsPackable>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\fishList.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
  </ItemGroup>
</Project>
